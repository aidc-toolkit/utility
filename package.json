{
  "name": "@aidc-toolkit/utility",
  "version": "1.0.43-alpha",
  "description": "Foundational utilities for AIDC Toolkit",
  "type": "module",
  "main": "dist/index.js",
  "homepage": "https://aidc-toolkit.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidc-toolkit/utility.git"
  },
  "bugs": {
    "url": "https://github.com/aidc-toolkit/utility/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Kevin Dean",
    "email": "Kevin.Dean@datadevelopment.com",
    "url": "https://www.linkedin.com/in/kdean"
  },
  "scripts": {
    "lint": "eslint",
    "tsc-src": "tsc --project tsconfig-src.json",
    "build:non-prod": "rimraf dist tsconfig-src.tsbuildinfo && npm run tsc-src -- --declarationMap --sourceMap",
    "build:alpha": "npm run build:non-prod && tsc --project tsconfig-config.json --noEmit",
    "build:beta": "npm run build:non-prod",
    "build:prod": "npm run tsc-src -- --noEmit && tsup --tsconfig tsconfig-tsup.json",
    "build:doc": "npm run build:non-prod",
    "test": "tsc --project tsconfig-test.json --noEmit && vitest run"
  },
  "devDependencies": {
    "@aidc-toolkit/dev": "1.0.41-alpha.202601291729",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@aidc-toolkit/core": "1.0.42-alpha.202601291729"
  }
}
